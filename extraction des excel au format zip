#importation des modules nécessaires
import requests, zipfile
from io import BytesIO
print('Downloading started')

#lien des données
url = "https://www.statistiques.developpement-durable.gouv.fr/sites/default/files/2021-02/parc_par_departement_dpe_2018.zip"
filename = url.split('/')[-1]

# téléchargement des données à partir du lien
req = requests.get(url)
print('Downloading Completed')

# extraction du document zip
zipfile= zipfile.ZipFile(BytesIO(req.content))
zipfile.extractall("https://github.com/f-b-l/python2A.git")
